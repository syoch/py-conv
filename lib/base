#pragma once
#ifndef __BASE_H__
#define __BASE_H__

#include <string>
#include <any>
#include <utility>

#define str std::string
class Any: public std::any{
    using std::any::any;

public:
    const char* toString(){
        static str tmp = std::any_cast<str>(*this);
        return tmp.c_str();
    }
};

#endif