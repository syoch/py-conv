#pragma once
#ifndef _OS_
#define _OS_

#include "genericpath"
#include "base"

class os
{
public:
	_path path;
	void mkdir(Any name, bool exist_ok);
	void rmdir(Any name);
	void renames(Any old, Any _new);
};

class OSError
{
private:
	Any msg;

public:
	OSError(Any msg) : msg(msg) {}
	Any what() { return msg; }
};

#endif // !_OS_